<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gson.room_store.model.mapper.customers.DemandModelMapper">
  <resultMap id="BaseResultMap" type="cn.gson.room_store.model.pojos.customers.DemandModelKey">
    <id column="model_id" jdbcType="INTEGER" property="modelId" />
    <id column="demand_id" jdbcType="INTEGER" property="demandId" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="cn.gson.room_store.model.pojos.customers.DemandModelKey">
    delete from demand_model
    where model_id = #{modelId,jdbcType=INTEGER}
      and demand_id = #{demandId,jdbcType=INTEGER}
  </delete>
  <insert id="insert" keyColumn="model_id" keyProperty="modelId" parameterType="cn.gson.room_store.model.pojos.customers.DemandModelKey" useGeneratedKeys="true">
    insert into demand_model (demand_id)
    values (#{demandId,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" keyColumn="model_id" keyProperty="modelId" parameterType="cn.gson.room_store.model.pojos.customers.DemandModelKey" useGeneratedKeys="true">
    insert into demand_model
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="demandId != null">
        demand_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="demandId != null">
        #{demandId,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>

  <insert id="batchAdd" parameterType="java.util.List">
    insert into demand_model values
    <foreach collection="list" item="p" index="i" separator=",">
        (#{p.modelId},#{p.demandId})
    </foreach>
  </insert>

  <select id="SelectedModelIntentionList" resultType="java.lang.Integer">
    select model_id from demand_model where demand_id=#{demandId}
  </select>

  <delete id="deleteBydemandId">
    delete from demand_model
    where demand_id = #{demandId,jdbcType=INTEGER}
  </delete>
</mapper>